<%= form_for :user , url: users_path do |f| %>

<h1> Create </h1>

<table>
	<tr>
		<td><%= f.label :email %></td>
		<td><%= f.text_field :email %></td>
	</tr>

	<tr>
		<td><%= f.label :password %></td>
		<td><%= f.text_field :password %></td>
	</tr>
	<tr> <td><%= f.submit %></td>
	</tr>
</table>

<% end %>