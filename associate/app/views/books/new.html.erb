<ul class="nav nav-tabs" role="tablist">
<li><%= link_to 'View all Books', lib_books_path %></li>
  <li class="active"><%= link_to 'New Books', new_lib_book_path %></li>
</ul>



<%= form_for :book, url: lib_books_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :author %><br>
    <%= f.text_area :author %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>